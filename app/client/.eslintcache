rect && <Redirect to=\"/dashboard\" />}\n\n      {/* Material UI says it increases the priority of the hero background image */}\n      {\n        <img\n          style={{ display: \"none\" }}\n          src={post.image_url}\n          alt={post.title}\n        />\n      }\n      <Link to={`./blog/${post._id}`} className={classes.continueReading}>\n        <Box width={1} height={1} className={classes.heroPost}>\n          <h1 className={classes.heroTitle}>{post.title}</h1>\n          <h5 className={classes.heroDate}>{getDate(post.created_at)}</h5>\n          <h3 className={classes.heroByline}>{post.byline}</h3>\n        </Box>\n      </Link>\n      {isSuper && (\n        <Chip\n          size=\"small\"\n          label=\"edit\"\n          className={classes.editChip}\n          onClick={() => handleEdit()}\n        />\n      )}\n    </Paper>\n  );\n}\n","/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/HeroImage/HeroImage.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/ResultsGrid/ResultsGrid.js",["178","179","180","181"],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/TitleBar/TitleBar.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/Navbar/SearchField.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/icons/BoxEmpty.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/icons/BoxFull.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/LoginModal/LoginForm.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Dashboard/AddRecords.js",["182","183"],"import React, { useState } from \"react\";\nimport { useForm } from \"react-hook-form\";\nimport { useGlobal } from \"../../context/GlobalState\";\nimport { API } from \"../../util/fetch\";\nimport useStyles from \"./DashboardStyles\";\n\nexport default function AddRecords() {\n  const classes = useStyles();\n  const globe = useGlobal();\n 