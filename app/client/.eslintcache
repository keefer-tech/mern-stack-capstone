classes.totalsAndPaymentContainer}>\n        <CartTotals />\n\n        <Checkout\n          showCardForm={showCardForm}\n          setShowCardForm={setShowCardForm}\n          cart={cart}\n        />\n      </Card>\n    </Container>\n  );\n}\n","/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Cart/CartStyles.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Cart/CartItem.js",[],{"ruleId":"79","severity":1,"message":"80","line":1,"column":10,"nodeType":"81","messageId":"82","endLine":1,"endColumn":13},{"ruleId":"79","severity":1,"message":"83","line":1,"column":15,"nodeType":"81","messageId":"82","endLine":1,"endColumn":24},{"ruleId":"79","severity":1,"message":"84","line":1,"column":26,"nodeType":"81","messageId":"82","endLine":1,"endColumn":30},{"ruleId":"79","severity":1,"message":"85","line":9,"column":5,"nodeType":"81","messageId":"82","endLine":9,"endColumn":16},{"ruleId":"79","severity":1,"message":"80","line":2,"column":3,"nodeType":"81","messageId":"82","endLine":2,"endColumn":6},{"ruleId":"79","severity":1,"message":"83","line":4,"column":3,"nodeType":"81","messageId":"82","endLine":4,"endColumn":12},{"ruleId":"79","severity":1,"message":"84","line":5,"column":3,"nodeType":"81","messageId":"82","endLine":5,"endColumn":7},{"ruleId":"79","severity":1,"message":"86","line":10,"column":8,"nodeType":"81","messageId":"82","endLine":10,"endColumn":13},{"ruleId":"79","severity":1,"message":"87","line":12,"column":8,"nodeType":"81","messageId":"82","endLine":12,"endColumn":13},{"ruleId":"79","severity":1,"message":"88","line":13,"column":10,"nodeType":"81","messageId":"82","endLine":13,"endColumn":13},{"ruleId":"79","severity":1,"message":"89","line":88,"column":18,"nodeType":"81","messageId":"82","endLine":88,"endColumn":22},{"ruleId":"79","severity":1,"message":"90","line":88,"column":24,"nodeType":"81","messageId":"82","endLine":88,"endColumn":32},{"ruleId":"79","severity":1,"message":"91","line":93,"column":43,"nodeType"